---
import Layout from "../layouts/Layout.astro";
import SelfIntroduction from "../components/astro/SelfIntroduction.astro";
import LatestPosts from "../components/astro/LatestPosts.astro";
import RandomPhotos from "../components/astro/RandomPhotos.astro";

const title = "Yeti's Planet";
---

<Layout title={title} h1={false}>
  <h1 class="mb-3 text-3xl">Home</h1>
  <div class="grid gap-4">
    <SelfIntroduction />
    <LatestPosts />
    <RandomPhotos client:load />
  </div>
</Layout>
